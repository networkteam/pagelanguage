{
  "name": "networkteam/pagelanguage",
  "type": "typo3-cms-extension",
  "description": "Set the language fallbackType via page field",
  "homepage": "https://networkteam.com",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3",
    "extension",
    "sys_language_mode",
    "sitehandling",
    "middleware"
  ],
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4 || ^11.5"
  },
  "autoload": {
    "psr-4": {
      "Networkteam\\Pagelanguage\\": "Classes"
    }
  },
  "replace": {
    "networkteam/pagelanguage": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pagelanguage"
    }
  }
}
